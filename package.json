{
  "name": "internetips",
  "version": "0.0.2",
  "description": "A tooltip library without the whole kitchen sink",
  "main": "dist/node/index.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
